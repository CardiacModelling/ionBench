[[model]]
name: Courtemanche 1998 IKur
author: Matt Owen
# Initial values
ikur.ua = 4.96e-3
ikur.ui = 0.999

#
# Simulation engine variables
#
[engine]
time = 0 [ms] bind time
    in [ms]
pace = -80 [mV] bind pace
    in [mV]

#
# Membrane potential
#
[membrane]
V = engine.pace
    desc: membrane potential
    in [mV]
EK = R*T/F*log(Ko/Ki)
    in [mV]
Ko = 5 [mmol]
    in [mmol]
Ki = ikur.p25
    in [mmol]
F =  9.6485e4 [C/mol]
    in [C/mol]
R = 8314.472 [mJ/mol/K]
    in [mJ/mol/K]
T = 295 [K] #22C (from 20C-25C)
    in [K]

[ikur]
use membrane.V
IKur = gkur * ua^3 * ui * (V - membrane.EK)
    in [A/F]
gkur = if(gkur4==0,gkur1, gkur1+gkur2/(1+exp((V+gkur3)/gkur4)))
dot(ua) = (inf - ua) / tau
    inf = 1/(1+exp((V+uam1)/uam2)) in [1] #Steady state autodiff requires that there are no 'if()'s or 'piecewise()'s in the inf so can't catch xrm2==0
    tau = abs(1 [ms*mV] / ((alphaua + betaua)*uakq10)) in [ms] #Given units don't match. Scaling factor introduced to make tau have units of ms rather than mV
    alphaua = if(uaa5==0 or uaa3==0,0, uaa1/(exp((V+uaa2)/uaa3)+exp((V+uaa4)/uaa5))) in [mV]
    betaua = if(uab3==0,0, 0.65 [1]/(uab1+exp((V+uab2)/uab3))) in [mV]
dot(ui) = (inf - ui) / tau
    inf = 1/(1+exp((V+uim1)/uim2)) in [1]
    tau = abs(1 [ms*mV] / ((alphaui + betaui)*uikq10)) in [ms] #Given units don't match. Scaling factor introduced to make tau have units of ms rather than mV
    alphaui = if(uia4==0,0,uia1/(uia2+exp((V+uia3)/uia4))) in [mV]
    betaui = exp((V+uib1)/uib2) in [mV]
p1 = 0.65 [1]
    in [1]
